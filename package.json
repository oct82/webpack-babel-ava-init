{
  "name": "webpack-ava-example",
  "version": "0.0.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "engine-strict": true,
  "scripts": {
    "check": "npm-check -u",
    "lint": "eslint .",
    "ava": "ava src/test/**/*.spec.js",
    "watch": "npm run ava -- --watch ",
    "test": "npm run lint && npm run ava",
    "webpack": "webpack --progress --colors --hot",
    "publish": "npm run test && npm run webpack",
    "clean": "rm -rf docs/",
    "start": "webpack-dev-server"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "0.16.0",
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.0",
    "babel-loader": "6.2.7",
    "babel-preset-es2015": "6.18.0",
    "eslint": "3.9.1",
    "eslint-config-standard": "6.2.1",
    "eslint-plugin-promise": "3.3.1",
    "eslint-plugin-standard": "2.0.1",
    "html-webpack-plugin": "2.24.1",
    "npm-check": "5.4.0",
    "webpack": "1.13.3",
    "webpack-dev-server": "1.16.2"
  }
}
